{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "GiftopiaAdmin": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/giftopia-admin",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/animate.css",
              "src/assets/css/baguetteBox.min.css",
              "src/assets/css/bootstrap-grid.css",
              "src/assets/css/bootstrap-grid.min.css",
              "src/assets/css/bootstrap-reboot.css",
              "src/assets/css/bootstrap-reboot.min.css",
              "src/assets/css/bootstrap.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/calendar.css",
              "src/assets/css/calendar.min.css",
              "src/assets/css/color_2.css",
              "src/assets/css/custom.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/font-awesome.css",
              "src/assets/css/jquery.fancybox.css",
              "src/assets/css/jquery.fancybox.min.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/perfect-scrollbar.css",
              "src/assets/css/responsive.css",
              "src/assets/css/semantic.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/analyser.js",
              "src/assets/js/animate.js",
              
              "src/assets/js/calendar.js",
              "src/assets/js/calendar.min.js",
              "src/assets/js/chart_custom_style1.js",
              "src/assets/js/chart_custom_style2.js",
              "src/assets/js/Chart.bundle.js",
              "src/assets/js/Chart.bundle.min.js",
              "src/assets/js/Chart.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/custom_chart.js",
              "src/assets/js/custom.js",
              "src/assets/js/jquery-3.3.1.min.js",
              "src/assets/js/jquery.fancybox.js",
              "src/assets/js/jquery.fancybox.min.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/owl.carousel.js",
              "src/assets/js/perfect-scrollbar.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/semantic.min.js",
              "src/assets/js/utils.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "GiftopiaAdmin:build:production"
            },
            "development": {
              "buildTarget": "GiftopiaAdmin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "GiftopiaAdmin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
